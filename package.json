{
  "name": "dacha",
  "version": "0.17.0",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "require": "./build/index.js",
      "import": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./events": {
      "require": "./build/events/index.js",
      "import": "./build/events/index.js",
      "types": "./build/events/index.d.ts"
    },
    "./renderer": {
      "types": "./build/contrib/systems/renderer/types.d.ts"
    }
  },
  "dependencies": {
    "pixi.js": "8.13.2",
    "uuid-random": "1.0.9"
  },
  "devDependencies": {
    "@eslint/js": "9.31.0",
    "@types/jest": "29.5.11",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-import": "2.32.0",
    "globals": "^16.3.0",
    "husky": "7.0.4",
    "jest": "29.7.0",
    "prettier": "3.6.2",
    "ts-jest": "29.1.1",
    "typedoc": "^0.28.13",
    "typescript": "5.3.3",
    "typescript-eslint": "8.37.0"
  },
  "sideEffects": false,
  "files": [
    "build",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:esm",
    "build:clean": "rm -rf build",
    "build:esm": "tsc --project tsconfig.esm.json",
    "docs": "typedoc",
    "lint": "eslint .",
    "test": "jest",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "description": "dacha is a JavaScript game engine for building games in browser.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michailRemmele/dacha.git"
  },
  "author": "Mikhail Remmele",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michailRemmele/dacha/issues"
  },
  "homepage": "https://github.com/michailRemmele/dacha#readme"
}
